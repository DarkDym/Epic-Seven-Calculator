MyScreenManager:
    MainScreen:
    SelectCharScreen:
    BuildEquipScreen:

<MainScreen>:
    name: "main_screen"

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        Label:
            text: "First Screen"
            font_size: 32
        
        Button:
            text: "Char Select"
            font_size: 32
            on_release: 
                app.root.current = "select_char_screen"
                root.manager.transition.direction = "left"
        Button:
            text: "PRINT"
            font_size: 32
            on_release:
                root.print_information()
                # app.root.current = "select_char_screen"
                # root.manager.transition.direction = "left"

<SelectCharScreen>:
    name: "select_char_screen"
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        scroll_type: ['bars', 'content']
        GridLayout:
            # orientation: "vertical"
            # size: root.width, root.height
            id: char_s
            # cols: 4
            # pos_hint: {"center_x": 0.5, "center_y": 0.5}
            spacing: 10 
            size_hint: 0.6,0.5
            # size_hint_y: None
            height: self.minimum_height
            size_hint_y: None
            # width: self.minimum_width
            cols: 4
            row_default_height: '255dp'
            col_default_width: '450dp'
            # row_default_width: '225dp'
            row_force_default: True
            # col_force_default: True
            # spacing: 0, 0
            # padding: 0, 0
        
        

        # Label:
        #     text: "Char Screen"
        #     font_size: 32
        
        # Button:
        #     text: "Go Back"
        #     font_size: 32
        #     on_release: 
        #         app.root.current = "main_screen"
        #         root.manager.transition.direction = "right"

